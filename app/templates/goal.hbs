<div class="header">
  <ul class="header-list">
      <li class="header-icons"><a>{{fa-icon "arrow-circle-down"}}</a></li>
      <li class="header-icons">{{fa-icon "lightbulb-o"}}</li>
    {{#link-to 'account'}}
      <li class="header-icons">{{fa-icon "fa-user"}}</li>
    {{/link-to}}
  </ul>




</div>

<div class="left-container">
<ul class="list">
  {{#each goal in model}}
    <li class="goal">{{fa-icon "fa-square-o"}}{{goal.title}}</li>
  {{/each}}
  {{#link-to 'create'}}
    <div class="create-button">
      {{fa-icon "fa-plus-circle"}}
    </div>
  {{/link-to}}
</ul>
</div>

<div class="right-container">
</div>

{{outlet}}
